{
    "task": "traffic_state_pred",
    "model": "MCK_GWN",
    "dataset": "NYCTAXI20140103",

    "exp_id": 100,

    "use_mh_adj": true,
    "ke_dim": 32,
    "max_hop": 1,
    "sparsity": 0.02,
    "load_external": true,
    "add_time_in_day": true,
    "add_day_in_week": true,

    "max_epoch": 500,
    "batch_size": 32,
    "patience": 15,
    "use_early_stop": true,
    "cache_dataset": false,
    "output_window": 12,
    "metrics": ["masked_MAE", "masked_RMSE", "masked_MAPE"]

}