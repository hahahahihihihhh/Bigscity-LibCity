{
    "task": "traffic_state_pred",
    "model": "DMKG_GNN",
    "dataset": "NYCTAXI20140103",

    "exp_id": 19,
    "n_layers": 3,
    "sparsity": 0.02,
    "ke_dim": 16,
    "load_external": true,
    "add_time_in_day": true,
    "add_day_in_week": true,

    "without_spatial_enhanced_KG": false,
    "without_dynamic_meteorological_KG": false,
    "without_spatial_enhanced": false,
    "without_attribute_enhanced": false,
    "without_time_attention": false,
    "without_state_propagation": false,

    "without_adaptive_skip_connection": true,

    "max_epoch": 500,
    "batch_size": 32,
    "patience": 15,
    "use_early_stop": true,
    "cache_dataset": false,
    "input_window": 12,
    "output_window": 12,
    "metrics": ["masked_MAE", "masked_RMSE", "masked_MAPE"]

}